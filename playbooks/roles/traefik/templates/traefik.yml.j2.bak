entryPoints:
  web:
    address: ":80"
  websecure:
    address: ":443"

providers:
  docker: {}
  file:
    filename: /opt/traefik/dynamic.yml

api:
  dashboard: true
  insecure: false

certificatesResolvers:
  cloudflare:
    acme:
      email: "{{ traefik_email }}"
      storage: "{{ traefik_acme_storage }}"
      dnsChallenge:
        provider: cloudflare
        resolvers:
          - "1.1.1.1:53"
          - "8.8.8.8:53"
