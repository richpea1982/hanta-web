http:
  middlewares:
    redirect-https:
      redirectScheme:
        scheme: https
        permanent: true

  routers:
    traefik-dashboard:
      rule: "Host(`proxy.{{ traefik_domain }}`)"
      service: "api@internal"
      entryPoints:
        - websecure
      tls:
        certResolver: cloudflare
      middlewares:
        - redirect-https
